{
    "configurations": [
        {
            "name": "Win32",
            "browse": {
                "path": [
                    "${env:MINGW64_ROOT}/include/c++",
                    "${env:MINGW64_ROOT}/include",
                    "${env:MINGW64_ROOT}/lib"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "includePath": [
                "${workspaceFolder}/**",
                "${env:MINGW64_ROOT}/include/miniupnpc"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "SPDLOG_FMT_EXTERNAL"
            ],
            "compilerPath": "${env:MINGW64_ROOT}/bin/clang++.exe",
            "cStandard": "c11",
            "cppStandard": "c++20",
            "intelliSenseMode": "clang-x64"
        }
    ],
    "version": 4
}